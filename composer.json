{
    "name": "akicreative/akiforms",
    "description": "Bootstrap integrated form generator.",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Steven Ishiwara",
            "email": "info@akicreative.net"
        }
    ],
    "version": "2.0.0",
    "minimum-stability": "dev",
    "require": {
        "intervention/image": "^2.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "AkiCreative\\AkiForms\\": "src/"
        },
        "classmap": [

            "src/Models"

        ]
    },
    "scripts": {
        "post-update-cmd": [
            "rsync -a vendor/akicreative/akiforms/resources/views/components/aki/ resources/views/components/aki/"
        ]
    },

    "extra": {
      "laravel": {
        "providers": [
          "AkiCreative\\AkiForms\\AkiFormsServiceProvider"
        ]
      }
    }
}
